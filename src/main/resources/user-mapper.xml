<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ru.dr.fix.mappers.UserMapper">

    <select id="findAllUsers" resultType="User">
        select * from users
    </select>

    <insert id="addUser" parameterType="ru.dr.fix.models.User" useGeneratedKeys="true" keyProperty="id" keyColumn="id">
        insert into users(name,login, password) values(#{name},#{login},#{password})
    </insert>

</mapper>